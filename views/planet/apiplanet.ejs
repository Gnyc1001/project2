<% include ../partials/boilerplate %>

<form method="POST" action="/planet/" class="planetform">
  <input type="text" name="pname" value="<%= locals.pname %>" />
  <input type="number" name="population" value="<%= locals.population %>" />
  <input type="text" name="climate" value="<%= locals.climate %>" />
  <input type="text" name="terrain" value="<%= locals.terrain %>" />
  <input type="text" name="control" value="<%= locals.control %>" />
  <input type="submit" class='displayadd' value="Add Planet" /><br>
  <a href="/planet/" class='displayadd'>Cancel Planet</a>
</form>

<% include ../partials/end %>
